+  console.info('hashchange', videoID, window.sponsorblock, window.sponsorblock ? window.sponsorblock.videoID : null, needsReload);
 
   if (needsReload) {
-    if (sponsorblock) {
+    if (window.sponsorblock) {
       try {
-        sponsorblock.destroy();
